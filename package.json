    "@react-navigation/native-stack": "^6.9.22",
    "buffer": "~6.0.3",
    "expo": "~53.0.9",
    "expo-crypto": "~14.1.4",
    "expo-dev-client": "~5.1.8",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-native": "0.79.2",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-paper": "~5.14.4",
    "react-native-safe-area-context": "5.4.0",
